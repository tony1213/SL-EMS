        <activity android:name="com.overtech.ems.activity.parttime.tasklist.ScanCodeActivity" />
        <activity android:name="com.overtech.ems.activity.parttime.grabtask.PackageDetailActivity" />
        <activity android:name="com.overtech.ems.activity.parttime.common.ElevatorDetailActivity" />
        <activity android:name="com.overtech.ems.activity.parttime.personal.PersonalAccountListActivity" />
        <activity android:name="com.overtech.ems.activity.parttime.personal.PersonalBoundsActivity" />
        <activity android:name="com.overtech.ems.activity.parttime.personal.PersonalChangePhoneNoActivity" />
