    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name="com.overtech.ems.activity.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.overtech.ems.activity.common.SplashActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="pRtgsRZyxmePt5lPifOnVSS4" />

        <activity android:name="com.overtech.ems.activity.BaseActivity" />
        <activity
            android:name="com.overtech.ems.activity.common.LoginActivity"
            android:launchMode="singleTask" >
        </activity>
        <activity
            android:name="com.overtech.ems.activity.parttime.MainActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity android:name="com.overtech.ems.activity.common.LostPasswordActivity" />
        <activity android:name="com.overtech.ems.activity.common.ResetPasswordActivity" />
        <activity android:name="com.overtech.ems.activity.common.ResetPasswordSuccessActivity" />
        <activity android:name="com.overtech.ems.activity.common.RegisterActivity" />
